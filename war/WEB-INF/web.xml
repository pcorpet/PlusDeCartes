<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
  	<servlet-name>carte</servlet-name>
  	<servlet-class>net.corpet.plusdecartes.serveur.CarteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>createur</servlet-name>
    <servlet-class>net.corpet.plusdecartes.serveur.CreateurServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>carte</servlet-name>
  	<url-pattern>/carte/*</url-pattern>
  </servlet-mapping>
<servlet-mapping>
  	<servlet-name>createur</servlet-name>
  	<url-pattern>/createur/*</url-pattern>
  </servlet-mapping>
</web-app>